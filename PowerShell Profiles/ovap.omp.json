{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.UserName}}{{if .Root}} :: root{{end}} :: {{.Shell}} :: {{.PWD}}",
  "palette": {
    "white": "#FFFFFF",
    "red": "#CC3802",
    "teal": "#047E84",
    "blue": "#0079be",
    "dark_blue2": "#025779",
    "purple": "#8c4ffd",
    "sky": "#86BBD8",
    "dark_blue": "#191f48",
    "orange": "#ec8848"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:white",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{if .Root}} \uf09c {{end}}{{if .SSHSession}}\ueba9 {{end}}{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:dark_blue2",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:purple",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:sky",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "p:sky",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ .Venv }} ",
          "type": "python"
        },
        {
          "type": "aws",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:white",
          "background": "p:sky",
          "template": " \udb83\ude0f {{.Profile}}{{if .Region}}@{{.Region}}{{end}} "
        },
        {
          "type": "az",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:white",
          "background": "p:sky",
          "template": " \uebd8 {{ .EnvironmentName }} "
        },
        {
          "background": "p:dark_blue",
          "foreground": "p:orange",
          "properties": {
            "time_format": "3:04 PM"
          },
          "style": "diamond",
          "template": " \uf0f4  {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
